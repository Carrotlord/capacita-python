sub factorial(n)
    when n <= 1
        return 1
    m = n - 1
    return n * factorial(m)
end

sub fibonacci(n)
    when n <= 1
        return n
    a = n - 1
    b = n - 2
    return fibonacci(a) + fibonacci(b)
end

print factorial(4)
print factorial(10)
print factorial(3) + factorial(5)

print fibonacci(4)
print fibonacci(8)
print fibonacci(2) + fibonacci(7)
